<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.ClientLoginController">
   <children>
      <BorderPane prefHeight="300.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="106.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="174.0" minWidth="10.0" prefWidth="174.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Username:" GridPane.halignment="CENTER" />
                  <Label text="Password:" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <TextField fx:id="usernameLabel" onAction="#enterPressed" GridPane.columnIndex="1" />
                  <PasswordField fx:id="passLabel" onAction="#enterPressed" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="loginButton" mnemonicParsing="false" onKeyTyped="#loginButtonClicked" onMouseClicked="#loginButtonClicked" text="Login" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                  <Button fx:id="createButton" mnemonicParsing="false" onMouseClicked="#createButtonClicked" text="Create User" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </center>
         <top>
            <Label text="TabChat" textAlignment="CENTER" textFill="#808a8f" BorderPane.alignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </top>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
